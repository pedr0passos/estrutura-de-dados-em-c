#include <stdlib.h>
#include <stdio.h>

#define MAX 10

typedef struct lista {
    int itens[MAX];
    int n;
} Lista;

void criarLista (Lista *l) {
    l->n = 0;
}

int estaVazia (Lista *l) {
    if (l->n == 0) {
        return 1;
    } else {
        return 0;
    }
}

int estaCheia ( Lista *l) {
    if (l->n == MAX) {
        printf("Lista estÃ¡ cheia!");        
        return 1;
    } else {
        return 0;
    }
}

void insereItem (Lista *l, int n ) {
    if ( !estaCheia(l)) {
        l->itens [l->n] = n;
        l->n++;
    }
    
}

void imprimeLista (Lista *l) {
    printf("Itens da Lista:\n");
    for (int i=0; i<l->n; i++) {

        printf("%d\n", l->itens[i]);
    }
}

void retiraItem (Lista *l, int p) {
    if ( !estaVazia(l)) {
        for ( int i=p; i<l->n; i++ ) {
            if ( i == p ) {
                l->itens[i-1] = l->itens[i];
                
            }
            l->itens[i-1];
        }
    }
}

int main () {
    
Lista lista;

criarLista(&lista);

insereItem(&lista, 15);
insereItem(&lista, 23);
insereItem(&lista, 30);

retiraItem(&lista, 1);
imprimeLista(&lista);


    return 0;
}